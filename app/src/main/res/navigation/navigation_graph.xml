<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_graph"
    app:startDestination="@id/startFragment2">
    <fragment
        android:id="@+id/startFragment2"
        android:name="com.example.tabugamekotlin.StartFragment"
        android:label="fragment_start"
        tools:layout="@layout/fragment_start" >
        <action
            android:id="@+id/action_startFragment2_to_gameFragment2"
            app:destination="@id/gameFragment2" />
        <action
            android:id="@+id/action_startFragment2_to_settingFragment"
            app:destination="@id/settingFragment" />
        <argument
            android:name="defaultTime"
            app:argType="long"
            app:nullable="false"
            android:defaultValue="60000L" />
        <argument
            android:name="passLimit"
            app:argType="integer"
            android:defaultValue="3" />
        <argument
            android:name="tabuLimit"
            app:argType="integer"
            android:defaultValue="3" />
        <argument
            android:name="finishScore"
            app:argType="integer"
            android:defaultValue="50" />
    </fragment>
    <fragment
        android:id="@+id/gameFragment2"
        android:name="com.example.tabugamekotlin.GameFragment"
        android:label="fragment_game"
        tools:layout="@layout/fragment_game" >
        <argument
            android:name="teamName1"
            app:argType="string" />
        <argument
            android:name="teamName2"
            app:argType="string" />
        <argument
            android:name="time"
            app:argType="long" />
        <argument
            android:name="pass"
            app:argType="integer" />
        <argument
            android:name="tabu"
            app:argType="integer" />
        <argument
            android:name="finish"
            app:argType="integer" />
    </fragment>
    <fragment
        android:id="@+id/settingFragment"
        android:name="com.example.tabugamekotlin.SettingFragment"
        android:label="fragment_setting"
        tools:layout="@layout/fragment_setting" >
        <action
            android:id="@+id/action_settingFragment_to_startFragment2"
            app:destination="@id/startFragment2" />
    </fragment>
</navigation>